{
  "name": "objectsites",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "cp": "cp dist/objectsites/index.html dist/index.html && cp src/package.json dist/package.json",
    "build": "ng build --prod --deploy-url /objectsites/  &&  npm run cp",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev": "npm uninstall @jacquesparis/objects-model @jacquesparis/objects-client @jacquesparis/objects-website @jacquesparis/objects-angular-forms && npm --save install ../objects-model ../objects-client ../objects-website ../objects-angular-lib/dist/objects-angular-forms",
    "update": "npm uninstall @jacquesparis/objects-model @jacquesparis/objects-client @jacquesparis/objects-website @jacquesparis/objects-angular-forms && npm --save install @jacquesparis/objects-model @jacquesparis/objects-client @jacquesparis/objects-website @jacquesparis/objects-angular-forms",
    "lib": "npm run update && npm run build && git add -A && git commit --amend -m \"lib\" && git push -f origin HEAD && npm publish dist --access=public"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false,
    "source-map-resolve": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@jacquesparis/objects-angular-forms": "^1.0.7",
    "@jacquesparis/objects-client": "^1.0.6",
    "@jacquesparis/objects-model": "^1.0.4",
    "@jacquesparis/objects-website": "^1.0.4",
    "@uirouter/angular": "^6.0.1",
    "@uirouter/core": "^6.0.4",
    "@uirouter/rx": "^0.6.5",
    "angular6-json-schema-form": "^8.0.0",
    "bootstrap": "4.1.1",
    "bootstrap-icons": "^1.1.0",
    "jexl": "^2.2.2",
    "jquery": "^3.5.1",
    "leaflet": "^1.7.1",
    "lodash-es": "^4.17.15",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^6.1.0",
    "ngx-bootstrap-icons": "^1.2.1",
    "ngx-schema-form": "^2.5.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "z-schema": "^4.2.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "umd-compat-loader": "^2.1.2"
  }
}
